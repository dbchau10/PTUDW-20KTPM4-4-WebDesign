    <div class="text-heading d-none d-lg-flex">
      <div class="text-white text-center">
        <div class="title">Gobe</div>
      </div>
      <form
        method="get"
        action="/"
        class="mb-3 w-50 rounded-pill bg-white search-form"
      >
        <h5 class="align-items-center d-flex ms-3 pt-1">
          <i class="fa-solid fa-location-dot text-black"></i>
        </h5>
        <select class="form-select form-select-lg w-40 border-0" name="start">
          <option selected>Điểm đi</option>
          <option value="">Hà Nội</option>
          <option value="">Quảng Ninh</option>
          <option value="">Ninh Bình</option>
        </select>
        <h5 class="align-items-center d-flex ms-3 pt-1">
          <i class="fa-solid fa-location-dot text-black"></i>
        </h5>
        <select class="form-select form-select-lg w-40 border-0" name="end">
          <option selected>Điểm đến</option>
          <option value="">Đà Nẵng</option>
          <option value="">Sài Gòn</option>
          <option value="">Khánh Hoà</option>
        </select>
        <input type="date" class="form-control border-0 w-10" name="date" />
        <button
          class="btn my-2 my-sm-0 btn-search"
          type="submit"
          onclick="location.href='/'"
        >
          <i class="fa-solid fa-magnifying-glass"></i>
        </button>
      </form>
    </div>
    <div class="form-heading d-lg-none">
      <form class="my-3 submit-form rounded bg-white p-2" action="/">
        <div class="d-flex flex-row">
          <h5 class="align-items-center d-flex ms-3">
            <i class="fa-solid fa-location-dot text-primary"></i>
          </h5>
          <select
